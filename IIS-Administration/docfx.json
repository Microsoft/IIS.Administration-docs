      "author": "shirhatti",
      "ms.author": "soshir"
    },
    "fileMetadata": {
      "ms.topic": {
        "iis-administration/api/**/**.md": "reference"
      }    
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
