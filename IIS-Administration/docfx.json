{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*swagger.json"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/iis-administration/breadcrumb/toc.json",
	    "extendBreadcrumb": "true",
      "searchScope": ["IIS"],
      "ms.prod": "iis",
      "ms.technology": "iis-administration",
